{
  "short_name": "Netflix Auth",
  "name": "Netflix Auth",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Netflix Auth",
    "default_icon": {
      "16": "favicon.ico",
      "48": "favicon.ico",
      "128": "favicon.ico"
    }
  },
  "permissions": ["cookies"],
  "host_permissions": ["<all_urls>"]
}
